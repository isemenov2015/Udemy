    "from bokeh.plotting import figure\n",
    "from bokeh.io import output_file, show\n",
    "import pandas as pd\n",
    "\n",
    "df = pd.read_csv(\"http://pythonhow.com/data/bachelors.csv\")\n",
    "#df.head()\n",
    "\n",
    "x = df[\"Year\"]\n",
    "y = df[\"Engineering\"]\n",
    "\n",
    "output_file(\"Exercise3.html\")\n",
    "f = figure()\n",
    "f.line(x, y)\n",
    "show(f)"
   ]
